    "snap:reset": "node scripts/snap-reset.js"
  },
  "dependencies": {

    "@types/node-fetch": "^2.6.12",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.848.0",
    "@types/express": "^5.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.5"
  }
}
